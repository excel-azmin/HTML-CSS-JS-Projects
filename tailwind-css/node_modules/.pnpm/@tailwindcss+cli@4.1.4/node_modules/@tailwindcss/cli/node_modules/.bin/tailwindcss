#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/azmin/Desktop/web-dev/HTML-CSS-JS-Projects/tailwind-css/node_modules/.pnpm/@tailwindcss+cli@4.1.4/node_modules/@tailwindcss/cli/dist/node_modules:/home/azmin/Desktop/web-dev/HTML-CSS-JS-Projects/tailwind-css/node_modules/.pnpm/@tailwindcss+cli@4.1.4/node_modules/@tailwindcss/cli/node_modules:/home/azmin/Desktop/web-dev/HTML-CSS-JS-Projects/tailwind-css/node_modules/.pnpm/@tailwindcss+cli@4.1.4/node_modules/@tailwindcss/node_modules:/home/azmin/Desktop/web-dev/HTML-CSS-JS-Projects/tailwind-css/node_modules/.pnpm/@tailwindcss+cli@4.1.4/node_modules:/home/azmin/Desktop/web-dev/HTML-CSS-JS-Projects/tailwind-css/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/azmin/Desktop/web-dev/HTML-CSS-JS-Projects/tailwind-css/node_modules/.pnpm/@tailwindcss+cli@4.1.4/node_modules/@tailwindcss/cli/dist/node_modules:/home/azmin/Desktop/web-dev/HTML-CSS-JS-Projects/tailwind-css/node_modules/.pnpm/@tailwindcss+cli@4.1.4/node_modules/@tailwindcss/cli/node_modules:/home/azmin/Desktop/web-dev/HTML-CSS-JS-Projects/tailwind-css/node_modules/.pnpm/@tailwindcss+cli@4.1.4/node_modules/@tailwindcss/node_modules:/home/azmin/Desktop/web-dev/HTML-CSS-JS-Projects/tailwind-css/node_modules/.pnpm/@tailwindcss+cli@4.1.4/node_modules:/home/azmin/Desktop/web-dev/HTML-CSS-JS-Projects/tailwind-css/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.mjs" "$@"
else
  exec node  "$basedir/../../dist/index.mjs" "$@"
fi
